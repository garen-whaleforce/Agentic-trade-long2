# Contrarian Alpha - Environment Variables
# Copy this file to .env and fill in your values

# =====================================
# External API Keys
# =====================================

# Earnings Call API
EARNINGSCALL_API_KEY=your_earningscall_api_key_here
EARNINGSCALL_API_URL=https://api.earningscall.com/v1

# Whaleforce Backtest API (SSOT for all performance metrics)
WHALEFORCE_BACKTEST_API_KEY=your_whaleforce_api_key_here
WHALEFORCE_BACKTEST_API_URL=https://api.whaleforce.com/v1

# LiteLLM / OpenAI / Anthropic
LITELLM_API_KEY=your_litellm_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# =====================================
# Database
# =====================================
DATABASE_URL=postgresql://user:password@localhost:5432/rocket_screener
# For SQLite (development): sqlite:///./data/rocket_screener.db

# =====================================
# Cache Storage
# =====================================
CACHE_DIR=./data/cache
# Optional: MinIO for object storage
MINIO_ENDPOINT=localhost:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_BUCKET=rocket-screener

# =====================================
# Application Settings
# =====================================

# Environment: development | staging | production
ENVIRONMENT=development

# Backend
BACKEND_HOST=0.0.0.0
BACKEND_PORT=8000
BACKEND_DEBUG=true

# Frontend
NEXT_PUBLIC_API_URL=http://localhost:8000/api

# =====================================
# LLM Configuration
# =====================================

# Default models
LLM_BATCH_SCORE_MODEL=gpt-4o-mini
LLM_FULL_AUDIT_MODEL=gpt-4o

# Cost limits
LLM_BATCH_SCORE_MAX_COST=0.01
LLM_FULL_AUDIT_MAX_COST=0.10

# Temperature (always 0 for determinism)
LLM_TEMPERATURE=0

# =====================================
# Strategy Configuration
# =====================================

# Thresholds (DO NOT CHANGE after 2026-01-01 freeze)
# Note: Production uses frozen config (configs/v9_g2_frozen.yaml) threshold=0.58
STRATEGY_SCORE_THRESHOLD=0.58
STRATEGY_EVIDENCE_MIN_COUNT=2
STRATEGY_WIN_RATE_TARGET=0.75

# Walk-forward periods
STRATEGY_TUNE_START=2017-01-01
STRATEGY_TUNE_END=2021-12-31
STRATEGY_VALIDATE_START=2022-01-01
STRATEGY_VALIDATE_END=2023-12-31
STRATEGY_TEST_START=2024-01-01
STRATEGY_TEST_END=2025-12-31
STRATEGY_PAPER_START=2026-01-01

# =====================================
# FMP (Financial Modeling Prep) API
# =====================================
FMP_API_KEY=your_fmp_api_key_here

# =====================================
# LINE Push Notifications
# =====================================
# Migrated to centralized line-push-service (2026-02-10)
# No config needed here â€” service runs at http://localhost:8730/api/push on gpu5090

# =====================================
# Logging
# =====================================
LOG_LEVEL=INFO
LOG_FORMAT=json
